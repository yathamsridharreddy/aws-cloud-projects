# CodeSync API Configuration
# Copy this file to .env and fill in your actual API keys
# DO NOT COMMIT .env file to git!

# LeetCode uses GraphQL - no API key needed but can add for tracking
LEETCODE_ENABLED=true

# HackerRank API Key (optional, can work without it)
HACKERRANK_ENABLED=true

# CodeChef credentials (optional, uses web scraping as fallback)
CODECHEF_ENABLED=true

# GeeksForGeeks (optional)
GFG_ENABLED=true

# Port configuration
PORT=3000

# Cache TTL in seconds
CACHE_TTL=600
